#Program
Word= input('Please enter a string ')
def most_frequent(string):
    d = dict()
    for key in string:
        if key not in d:
            d[key] = 1
        else:
            d[key] += 1
    return d

print (most_frequent(Word))
#Output :
Please enter a string MADMAN
{'M': 2, 'A': 2, 'D': 1, 'N': 1}
